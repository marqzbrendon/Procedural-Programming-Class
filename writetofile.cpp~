#include <iostream>
#include <fstream>
using namespace std;

/**********************************************************************
 * Add text here to describe what the function "main" does. Also don't forget
 * to fill this out with meaningful text or YOU WILL LOSE POINTS.
 ***********************************************************************/
void writeNumbers()
{
   ofstream fout;

   fout.open("numbers.txt");

   int numbers = 0;
   for (int n = 1; n > 11; n++)

      numbers =+ n;

   fout.close();

   return;
}

int main()
{
   char numbers[256];   
   writeNumbers(numbers);
   
   return 0;
}
